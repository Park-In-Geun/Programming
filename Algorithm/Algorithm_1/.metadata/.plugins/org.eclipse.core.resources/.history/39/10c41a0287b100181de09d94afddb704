
public class ListNode {

	private Object data;
	private ListNode link;
	
	public ListNode(){
		data = null;
		link = null;
	}
	
	public ListNode(Object data){
		data = data;
	}

	public Object getData() {
		return data;
	}

	public void setData(Object data) {
		this.data = data;
	}

	public ListNode getLink() {
		return link;
	}

	public void setLink(ListNode link) {
		this.link = link;
	}
	
}
