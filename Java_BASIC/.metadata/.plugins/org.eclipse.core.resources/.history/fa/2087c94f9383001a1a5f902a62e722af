package quiz2;

public class Cat extends Animal{
	int count;
	String name;
	
	Cat(){
		this("Anonymous");
	}
	
	Cat(String n){
		this.name = n;
		this.count += 1;
	}
	
	public void sound(){
		return "야~옹";
	}
	
	int getCountCat(){
		return this.count;
	}
	
	public String toString(){
		return this.name + "고양이";
	}
	
}
